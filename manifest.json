{
  "manifest_version": 3,
  "name": "Focus mode",
  "description": "Enable focus mode on Chrome docs",
  "version": "0.0.1",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "64": "images/icon-64.png"
  },
  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "64": "images/icon-64.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab"]
}
